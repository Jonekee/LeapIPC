# - Config file for the LeapIPC package
# It defines the following variables
#  LeapIPC_FOUND - indicates that the module was found
#  LeapIPC_INCLUDE_DIR - include directories

# Check if local build
if ("@CMAKE_CURRENT_BINARY_DIR@" STREQUAL CMAKE_CURRENT_LIST_DIR)
  set(LeapIPC_INCLUDE_DIR "@PROJECT_SOURCE_DIR@")
  include("${CMAKE_CURRENT_LIST_DIR}/LeapIPCTargets.cmake")
else()
  set(LeapIPC_INCLUDE_DIR "${CMAKE_CURRENT_LIST_DIR}/include")
  include("${CMAKE_CURRENT_LIST_DIR}/cmake/LeapIPCTargets.cmake")
endif()

set(LeapIPC_FOUND TRUE)
